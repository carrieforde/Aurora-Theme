{
  "name": "aurora-theme",
  "version": "1.0.0",
  "description": "A clean, modern React-powered WordPress theme.",
  "main": "app.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write --single-quote \"src/**/*.{js,jsx}\"",
    "format:sass": "prettier --write --tab-width=2 --use-tabs --single-quote=false \"src/sass/**/*.scss\"",
    "stylelint": "stylelint \"src/**/*.{scss,css}\"",
    "stylelint:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/aurora-theme.pot' --domain 'aurora-theme'"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carrieforde/Aurora-Theme.git"
  },
  "keywords": [
    "sass",
    "javascript",
    "es6",
    "eslint",
    "stylelint"
  ],
  "author": "Carrie Forde",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carrieforde/Aurora-Theme/issues"
  },
  "homepage": "https://github.com/carrieforde/Aurora-Theme#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "aurora-utilities": "^1.1.0",
    "autoprefixer": "^9.4.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.91.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.1",
    "react-feather": "^1.1.5",
    "sanitize.scss": "^1.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.5.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "wp-pot-cli": "^1.2.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
